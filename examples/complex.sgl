fun add(a: Int, b: Int) -> Int {
    return a + b;
}

fun do_work() -> Int {
    let i: Int = 0;
    let result: Int = 0;
    while (i < 10) {
        result = add(result, i);
        i = i + 1;
    }

    return result;
}


let work_result: Int = do_work();
println("Result: ", string(work_result));
